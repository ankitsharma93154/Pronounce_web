{
    "headers": [
      {
        "source": "/static/:path*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/_next/static/:path*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/:path(.+\\.(jpg|jpeg|gif|png|svg|ico|webp))",
